{
  "name": "ssl-validator",
  "version": "1.0.1",
  "description": "Validator for certificates, and keys",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "lint": "esw *.js lib test examples",
    "lint:fix": "esw --fix *.js lib test examples",
    "lint:watch": "esw --watch *.js lib test examples",
    "lint:staged": "lint-staged",
    "precommit": "lint-staged",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Losant/ssl-validator.git"
  },
  "keywords": [
