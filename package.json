{
  "name": "ssl-validator",
  "version": "1.0.5",
  "description": "Validator for certificates, bundles, and keys",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "lint": "esw . --ext .js",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "lint:changed": "lint-staged",
    "test": "mocha",
    "reinstall": "rm -rf node_modules && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Losant/ssl-validator.git"
  },
  "keywords": [
    "ssl",
    "openssl",
    "certificates",
    "keys"
  ],
  "author": "Losant IoT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Losant/ssl-validator/issues"
  },
  "engines": {
    "node": ">=8.3"
  },
  "lint-staged": {
    "*.js": "esw"
  },
  "dependencies": {
    "pem": "^1.14.3",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@losant/eslint-config-losant": "^1.3.10",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "mocha": "^6.2.2",
    "should": "^13.2.3"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
