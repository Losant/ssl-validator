  "author": "Losant IoT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Losant/ssl-validator/issues"
  },
  "engines": {
    "node": ">=8.3"
  },
  "lint-staged": {
    "*.js": "esw"
  },
  "dependencies": {
    "pem": "^1.12.3",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@losant/eslint-config-losant": "^1.3.2",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "mocha": "^5.0.5",
    "should": "^13.2.1"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node"
